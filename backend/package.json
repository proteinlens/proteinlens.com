{
  "name": "proteinlens-backend",
  "version": "1.0.0",
  "description": "ProteinLens backend - Azure Functions for meal photo analysis via GPT-5.1 Vision",
  "main": "dist/index.js",
  "type": "module",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "build": "tsc && node scripts/fix-esm-imports.mjs",
    "dev": "npm run db:setup && npm run build && func start",
    "postinstall": "prisma generate",
    "watch": "tsc --watch",
    "prestart": "npm run build",
    "start": "func start --typescript",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:unit": "vitest run tests/unit",
    "test:contract": "vitest run tests/contract",
    "test:integration": "vitest run tests/integration",
    "test:coverage": "vitest run --coverage",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "db:setup": "prisma migrate deploy && prisma db seed",
    "db:reset": "prisma migrate reset --force",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [
    "azure-functions",
    "gpt-vision",
    "meal-analysis",
    "nutrition"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@azure/communication-email": "^1.1.0",
    "@azure/functions": "^4.5.0",
    "@azure/identity": "^4.0.0",
    "@azure/storage-blob": "^12.17.0",
    "@prisma/client": "^5.8.0",
    "applicationinsights": "^3.12.1",
    "bcrypt": "^6.0.0",
    "jose": "^5.9.3",
    "nanoid": "^5.1.6",
    "prisma": "^5.8.0",
    "stripe": "^20.1.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.19.27",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "@vitest/coverage-v8": "^4.0.16",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "supertest": "^7.1.4",
    "ts-jest": "^29.1.1",
    "tsc-alias": "^1.8.16",
    "tsx": "^4.21.0",
    "typescript": "^5.3.3",
    "vitest": "^4.0.16"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  }
}
